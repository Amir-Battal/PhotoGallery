{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }  
    ],
    "routes": [
        {
            "src": "./public/uploads/(.*)",
            "dest": "uploads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}